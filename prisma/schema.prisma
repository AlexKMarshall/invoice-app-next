// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model InvoiceSummary {
  id         String   @id
  paymentDue DateTime
  clientName String
  total      Int      @default(0)
  status     String
}
