{
  "name": "next-prisma-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dev": "next build",
    "start": "next start",
    "start:db": "docker-compose up",
    "start:db:ci": "docker-compose -f docker-compose.ci.yml up -d",
    "stop:db:ci": "docker-compose -f docker-compose.ci.yml down",
    "lint": "eslint",
    "format": "prettier --write --ignore-path .gitignore",
    "test": "yarn test:client && yarn test:server",
    "test:cov": "yarn test:client --coverage && yarn test:server --coverage",
    "test:client": "jest --config ./jest.client.config.js",
    "test:server": "jest --config ./jest.server.config.js",
    "tsc": "tsc --noEmit",
    "cm": "cz",
    "prepare": "husky install",
    "postinstall": "yarn prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "db:migrate:dev": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:deploy": "yarn prisma migrate deploy",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.0",
    "@prisma/client": "3",
    "@react-aria/button": "^3.3.4",
    "@react-aria/checkbox": "^3.2.3",
    "@react-aria/dialog": "^3.1.4",
    "@react-aria/focus": "^3.4.1",
    "@react-aria/interactions": "^3.6.0",
    "@react-aria/menu": "^3.2.3",
    "@react-aria/overlays": "^3.7.2",
    "@react-aria/ssr": "^3.0.3",
    "@react-aria/utils": "^3.8.2",
    "@react-aria/visually-hidden": "^3.2.3",
    "@react-stately/checkbox": "^3.0.3",
    "@react-stately/menu": "^3.2.3",
    "@react-stately/overlays": "^3.1.3",
    "@react-stately/tree": "^3.2.0",
    "@vanilla-extract/babel-plugin": "^1.1.0",
    "@vanilla-extract/css": "^1.5.1",
    "@vanilla-extract/css-utils": "^0.1.1",
    "@vanilla-extract/dynamic": "^2.0.0",
    "@vanilla-extract/next-plugin": "^1.0.0",
    "@vanilla-extract/recipes": "^0.1.0",
    "@vanilla-extract/webpack-plugin": "^2.1.0",
    "date-fns": "^2.23.0",
    "mini-css-extract-plugin": "^2.3.0",
    "nanoid": "^3.1.25",
    "next": "^11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "react-query": "^3.19.6",
    "sharp": "^0.29.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "zod": "^3.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@storybook/addon-actions": "^6.4.0-beta.0",
    "@storybook/addon-essentials": "^6.4.0-beta.0",
    "@storybook/addon-links": "^6.4.0-beta.0",
    "@storybook/builder-webpack5": "^6.4.0-beta.0",
    "@storybook/manager-webpack5": "^6.4.0-beta.0",
    "@storybook/react": "^6.4.0-beta.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/pg": "^8.6.1",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "lint-staged": ">=10",
    "msw": "^0.34.0",
    "next-page-tester": "^0.28.0",
    "next-test-api-route-handler": "^2.2.0",
    "pg": "^8.7.1",
    "prettier": "^2.3.2",
    "prisma": "3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "type-fest": "^2.0.0",
    "typescript": "^4.3.5",
    "webpack": "5",
    "whatwg-fetch": "^3.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
